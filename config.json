{
	"data": {
		"filename": "2hdata.csv",
		"recentName": "2hdatarecent.csv",
		"columns": [
			"close",
			"volume",
			"ewm8","ewm13","ewm21","ewm55","StotchRSI"
			
		],
		"sequence_length": 30,
		"train_test_split": 0.9,
		"normalise": true
	},
	"training": {
		"epochs": 31,
		"batch_size": 30
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [

			{
				"type": "lstm",
				"neurons": 400,
				"input_timesteps": 29,
				"input_dim": 7,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 400,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 400,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}
